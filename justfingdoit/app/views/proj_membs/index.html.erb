<%- model_class = ProjMemb.new.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:projects_id) %></th>
      <th><%= model_class.human_attribute_name(:members_id) %></th>
      <th><%= model_class.human_attribute_name(:details) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @proj_membs.each do |proj_memb| %>
      <tr>
        <td><%= link_to proj_memb.id, proj_memb_path(proj_memb) %></td>
        <td><%= proj_memb.projects_id %></td>
        <td><%= proj_memb.members_id %></td>
        <td><%= proj_memb.details %></td>
        <td><%=l proj_memb.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_proj_memb_path(proj_memb), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      proj_memb_path(proj_memb),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_proj_memb_path,
            :class => 'btn btn-primary' %>
